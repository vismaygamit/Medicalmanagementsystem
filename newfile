create table address(Fullname varchar(50),Mobile varchar(10),quantity int,pincode int,House_no varchar(50),landmark varchar(50),city varchar(20),state varchar(20));
create table medicine(id int,mname varchar(10),mdate date,Expiry_date date,description varchar(50),qty int,price double);
create table sigin(fname varchar(20),lname varchar(20),phone varchar(10),email varchar(20),password varchar(20),dob date,gender varchar(20));